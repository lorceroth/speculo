{
  "name": "speculo",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack",
    "watch": "cross-env NODE_ENV=development webpack -w",
    "publish": "cross-env NODE_ENV=production webpack",
    "build:plugin": "cross-env NODE_ENV=development webpack --config webpack.config.plugins.js",
    "watch:plugin": "cross-env NODE_ENV=development webpack --config webpack.config.plugins.js -w",
    "publish:plugin": "cross-env NODE_ENV=production webpack --config webpack.config.plugins.js",
    "serve": "http-server ./dist"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reset-css": "^4.0.1",
    "scriptjs": "^2.5.9"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/scriptjs": "^0.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
